<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/linearLayout18"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="16dp"></LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="550dp"
        android:layout_marginTop="1dp"
        android:orientation="vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout17">

        <Button
            android:id="@+id/button5"
            android:layout_width="311dp"
            android:layout_height="58dp"
            android:layout_margin="25dp"
            android:layout_marginStart="15dp"
            android:background="@drawable/button_shape8"
            android:text="나의 프로필" />

        <Button
            android:id="@+id/button4"
            android:layout_width="311dp"
            android:layout_height="58dp"
            android:layout_margin="25dp"
            android:layout_marginStart="15dp"
            android:background="@drawable/button_shape9"
            android:text="개인정보 처리방침" />

        <Button
            android:id="@+id/button6"
            android:layout_width="311dp"
            android:layout_height="58dp"
            android:layout_margin="25dp"
            android:layout_marginStart="15dp"
            android:background="@drawable/button_shape10"
            android:text="고객센터" />

        <Button
            android:id="@+id/button8"
            android:layout_width="311dp"
            android:layout_height="58dp"
            android:layout_margin="25dp"
            android:layout_marginStart="15dp"
            android:background="@drawable/button_shape11"
            android:text="회원 탈퇴" />

        <Button
            android:id="@+id/button_logout"
            android:layout_width="311dp"
            android:layout_height="58dp"
            android:layout_margin="25dp"
            android:layout_marginStart="15dp"
            android:background="@drawable/button_shape12"
            android:text="로그아웃" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout17"
        android:layout_width="373dp"
        android:layout_height="141dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="1dp"
        android:orientation="horizontal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout18">

        <LinearLayout
            android:layout_width="368dp"
            android:layout_height="106dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#ff9424"
                app:srcCompat="@drawable/chat" />

            <EditText
                android:id="@+id/editTextTextPersonName2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="별명" />

        </LinearLayout>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>